{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "author": "dvaJi",
  "keywords": [
    "graphql",
    "node.js",
    "manga"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvaJi/ReaderFront/issues"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "prettier": "prettier --single-quote --write 'src/**/*.js'",
    "setup": "yarn setup:install && yarn setup:db",
    "setup:install": "yarn install",
    "setup:db": "yarn db:migrate && yarn db:seed",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "start": "set NODE_ENV=development && nodemon src/index.js --exec babel-node",
    "start:prod": "export NODE_ENV=production && yarn build:prod && yarn start:server",
    "build:prod": "babel src -s -D -d build",
    "start:server": "node build/index.js",
    "release": "standard-version"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "5.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.1",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "nodemon": "1.17.5",
    "prettier": "1.13.7",
    "standard-version": "^4.4.0"
  },
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^5.0.0-alpha.6",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql2": "^1.5.3",
    "sequelize": "4.37.10",
    "sequelize-cli": "^4.0.0"
  }
}
